    color: #FFFFD0;
}
QTabBar::tab:selected  {
    color: #00FF00;
}
QTabBar::tab:hover {
    border: 2px solid #3DAEE9;
}
QHeaderView {
    background-color: #B0B0D0;
    border: 0px transparent #32414B;
    padding: 0px;
    margin: 0px;
    border-radius: 0px;
    font: 10pt "Lato Heavy";
}
