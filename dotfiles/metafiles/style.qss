##Blue White
QMainWindow {
    background: #606060;
}
QStatusBar {
    background: #606060;
    font: 10pt "Lato Heavy";
    color: #FFFFD0;
}
QPushButton, ActionButton, SystemToolButton, AxisToolButton {
    background: qlineargradient(spread:reflect, x1:0.5, y1:0, x2:0.5, y2:0.66, stop:0 rgba(240, 240, 240, 255), stop:0.6 rgba(20, 20, 20, 255));
    border: 2px solid black;
    border-radius: 4px;
    font: 10pt "Lato Heavy";
    color: #FFFFD0;
}
QPushButton:checked, QPushButton:pressed {
    color: #00FF00;
}
QPushButton:hover, ActionButton:hover, SystemToolButton:hover, AxisToolButton:hover {
    border: 2px solid #3DAEE9;
}
QPushButton:disabled, ActionButton:disabled {
    color: gray;
}
ActionButton#action_machine_on {
    color: #FF0000;
}
ActionButton#action_machine_on:checked {
    color: #00FF00;
}
ActionButton#action_estop {
    color: #00FF00;
}
ActionButton#action_estop:checked {
    color: #FF0000;
}
QLabel {
    font: 10pt "Lato Heavy";
    color: black;
}
QLabel#label_axis_x,  QLabel#label_axis_y,  QLabel#label_axis_z,  QLabel#label_axis_a {
    background: qlineargradient(spread:reflect, x1:0.5, y1:0, x2:0.5, y2:0.66, stop:0 rgba(240, 240, 240, 255), stop:0.6 rgba(20, 20, 20, 255));
    border: 2px solid black;
    border-radius: 4px;
    font: 20pt "Lato Heavy";
    color: #0080FF;
}
StatusLabel, StateLabel {
    background: rgb(250, 250, 250);
    font: 10pt "Lato Heavy";
    color: black;
}
QLineEdit {
    background: rgb(250, 250, 250);
    border: 1px solid;
    border-left-color: black;
    border-top-color: black;
    border-right-color: white;
    border-bottom-color: white;
    font: 10pt "Lato Heavy";
}
QCheckBox {
    font: 10pt "Lato Heavy";
}
QComboBox {
    border: 1px solid black;
    width: 20px;
    font: 10pt "Lato Heavy";
    color: black;
}
QGroupBox {
    border: 1px solid black;
    border-radius: 4px;
    margin-top: 16px;
    font: 10pt "Lato Heavy";
}
QGroupBox::title {
    subcontrol-origin: margin;
    padding: 0 3px;
    color: black;
}
QProgressBar {
    background-color: rgb(250, 250, 250);
    border: 1px solid black;
    border-radius: 4px;
    font: 9 pt "Lato Heavy";
}
QProgressBar::chunk {
    background: qlineargradient(x1: 0, y1: 0.2, x2: 1, y2: 1,
    stop: 0 #bbf, stop: 1 #55f);
}
QTabWidget::pane > QWidget {
    background: lightgray;
    border: none;
}
QTabWidget::tab-bar {
left: 2px; /* move to the right by 2px */
}
QTabBar::tab {
    background: qlineargradient(spread:reflect, x1:0.5, y1:0, x2:0.5, y2:0.66, stop:0 rgba(240, 240, 240, 255), stop:0.6 rgba(20, 20, 20, 255));    margin-top: 2px;
    margin-bottom: 4px;
    margin-left: 2px;
    margin-right: 2px;
    border: 2px solid black;
    border-radius: 4px;
